{
  "name": "ds-rag-backend",
  "version": "2.0.0",
  "description": "RAG System with Cloud Document Sources Integration",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsnd --respawn --clear src/app.ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/app.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "railway:build": "npm install --omit=dev && npx prisma generate && npm run build",
    "railway:start": "npx prisma migrate deploy && npm run start",
    "make-admin": "ts-node src/scripts/make-admin.ts",
    "generate-keys": "node generate-keys.js"
  },
  "keywords": [],
