    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.15.5",
    "@types/puppeteer": "^5.4.7",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.15.3",
    "@types/xml2js": "^0.4.14",
